<h1>Routes</h1>

<table border = "2">
  <tr>
    <th>Id</th>
    <th>Route Name</th>
    <th>Route Source</th>
    <th>Route Destination</th>
    <th>Details</th>
  </tr>
  <% @routes.each do |route| %>
    <tr>
      <td><%= route.id %></td>
      <td><%= route.route_name %></td>
      <td><%= route.route_source %></td>
      <td><%= route.route_destination %></td>
      <td><%= link_to "view", route %></td>
    </tr>
  <% end %>

</table>
<br>
<%= link_to "Create New Route", new_route_path  %>